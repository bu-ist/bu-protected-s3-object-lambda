{
    "env": {
        "node": true
    },
    "extends": ["airbnb-base"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/extensions": ["error", "ignorePackages"],
        "import/no-extraneous-dependencies": ["error", {"packageDir": ["./", "./src"]}]
    }
}
